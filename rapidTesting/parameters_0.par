&equations
MaterialFileName = 'material.yaml'
/

&IniCondition
/

&Boundaries
BC_fs = 1
BC_dr = 0
BC_of = 1
/

&SourceType
Type = 50  
FileName = 'source/source_chain.dat'                        ! input file.
/

&SpongeLayer
/
            
&MeshNml
MeshFile = 'mesh/coarse.h5'
meshgenerator = 'PUML'
/

&Discretization
CFL = 0.5
FixTimeStep = 5 
ClusteredLTS = 2 
/

&Output
OutputFile = 'output/current/output'
