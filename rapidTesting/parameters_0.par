!             |stress     |vel  
iOutputMask = 1 1 1 1 1 1 1 1 1 
TimeInterval =  5.
refinement = 1

! Free surface output
SurfaceOutput = 0
SurfaceOutputRefinement = 1
SurfaceOutputInterval = 0.5

! Receiver Output
printIntervalCriterion = 2
pickdt = 0.0200
pickDtType = 1
RFileName = 'mesh/receivers.dat'
/

&AbortCriteria
EndTime = 2.01 
/

&Analysis
/

&Debugging
/
